

{{> messages }}
{{#if username}}
<h2>{{ username }}'s profile</h2>
<table class="table">
  <tr>
    <td>First Name:</td>
    <td>{{ fname }}</td>
  </tr>
  <tr>
    <td>Last Name:</td>
    <td>{{ lastName }}</td>
  </tr>
  <tr>
    <td>Email:</td>
    <td>{{ email }}</td>
  </tr>
  <tr>
    <td>Department:</td>
    <td>{{ department }}</td>
  </tr>
</table>
{{/if}}
